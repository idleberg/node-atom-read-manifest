{
  "name": "atom-read-manifest",
  "version": "0.5.2",
  "description": "Read the manifest (package.json) of any installed Atom package",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./tsconfig.tsbuildinfo && tsc --pretty --project ./",
    "dev": "npm run start",
    "lint:fix": "tslint --fix --project ./",
    "lint": "tslint --project ./",
    "start": "tsc --watch --pretty --project ./",
    "test": "npm run lint"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-atom-read-manifest.git"
  },
  "keywords": [
    "atom",
    "atom package",
    "manifest",
    "package.json",
    "package manifest"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "caller-callsite": "^4.1.0",
    "find-up": "^5.0.0",
    "hasha": "^5.2.2",
    "read-pkg-up": "^7.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/node": "^14.11.8",
    "husky": "^4.3.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
